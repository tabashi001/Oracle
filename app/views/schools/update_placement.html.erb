        <li class="nav-divider"></li>
    </ul>
    <!-- /#menu -->
  </div>
  <!-- /#left -->
  <div id="content">
    <div class="outer">
      <div class="inner bg-light lter">
        <h4>Placements</h4>
        <hr>
        <%= form_for(@placement, url: school_update_placement_path) do |f| %>
          <div class="row vdiocont">    
            <div class="form-group margunrve">
              <label for="email">Title</label>
              <%=f.text_field :title, class: "form-control",placeholder: "Title" %>
            </div>
            <div class="form-group margunrve">
              <label for="pwd">Description</label>
              <%=f.text_area :description, class: "form-control",placeholder: "Description" %>
            </div>
            <div class="form-group margunrve">
              <label for="text">No of Company Visited</label>
              <%=f.text_field :companies_visited, class: "form-control",placeholder: "No of Companies Visited" %>
            </div>
            <div class="form-group margunrve">
              <label for="text">No of Student Placed</label>
              <%=f.text_field :students_placed, class: "form-control",placeholder: "Students Placed" %>
            </div>
            <div class="form-group margunrve">
              <label for="text">Total No of Students</label>
              <%=f.text_field :total_students, class: "form-control",placeholder: "Total Students" %>
            </div>
            <div class="form-group margunrve">
              <label for="text">year</label>
              <%=f.text_field :year, class: "form-control",placeholder: "Year" %>
            </div>
           <div class="form-group margunrve text-center">
            <%=f.submit "Submit", class: "btn btn-default"%>
            <button type="submit" class="btn btn-default">cancel</button>
          </div>
        </div>
      <%end%>   
      <hr>
      </div>
      <!-- /.inner -->
    </div>
    <!-- /.outer -->
  </div>
