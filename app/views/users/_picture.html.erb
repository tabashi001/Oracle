	<div id="pictures">
				<span class="overview">Images of <%= @college.name %></span>
				<hr>
				<div class="row imgVid">
					<% if @gallery.present? %>
					<% @gallery.each do |gallery|%>
					<div class="col-md-3">
					  <div class="thumbnail">
						<a href="/assets/building.jpg" target="_blank">
						  <!-- <img src="/assets/building.jpg" alt="Lights" style="width:100%"> -->
						  <%= image_tag gallery.picture,style: "width:100%"%>
						  <div class="caption">
							<p><%= gallery.title%></p>
						  </div>
						</a>
					  </div>
					</div>
					<%end%>
					<%else%>
					<p>No Images are present.....</p>
					<%end%>
				</div>
				<span class="overview">Videos of <%= @college.name %></span>
				<hr>
					<div class="row galVid">
						<div class="col-sm-12">
							<div class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
								<div class="vidBox">
									<%if @video.present?%>
									<% @video.each do |video|%>
									<iframe width="100%" height="250" src="<%= video.video %>" autoplay="false"></iframe> 
									<%end%>
									<%else%>
									<p>No Video are present....</p>
									<%end%>
								</div>
							</div>
						</div>
					</div>
			</div>