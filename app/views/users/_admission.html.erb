<div id="admission">
				<div class="container overviewBox">
					<p class="admsTex"><span class="shco"><%= @college.name%></span><%= @college.description %></p>
				</div>
				<div class="container tabOx">
					<div class="table-responsive">          
						<table class="table table-striped">
							<thead>
							  <tr>
								<th>COURSE NAME</th>
								<th>CATEGORY</th>
								<th>RANK</th>
							  </tr>
							</thead>
							<tbody>
								<% @course_detail.each do |course|%>
							  <tr>
								<td><%= course.degree_name.titleize%> in <%= course.course_name.titleize %></td>
								<% if course.course_type == true%>
								<td>Regular</td>
								<% elsif course.course_type == false%>
								<td>Correspondence</td>
								<%end%>
								<td>john@example.com</td>
							  </tr>
							 <%end%>
							</tbody>
						</table>
					</div>
				</div>	
				<div class="container overviewBox">
					<span class="overview">Contact Details of <%= @college.name%>,<%= @college.city.capitalize %></span>
								<hr>
								<div class="row ">
									<div class="col-sm-12">
										
										<div class="col-sm-12 col-xs-12 col-md-4 col-lg-4">
											<p class="contact">Contact No:&nbsp;<span class="Detastpd"> <%= @college.phone_no %></span></p>
											<p class="contact"> Email ID: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <span class="Detastpd"><%= @college.email %></span></p>
											<p class="contact"> Website:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class="Detastpd"><a target="_blank" class="HanD" href="<%= @college.website%>"><%= @college.website%></a></span></p>
											<p class="contact"> Address: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class="Detastpd"><%= @college.address %></span></p>
										</div>
										<div class="col-sm-12 col-xs-12 col-md-8 col-lg-8">
										<div id="googleMap" style="width:100%;height:400px;"></div>
										</div>
									</div>
								</div>
				</div>
			</div>

