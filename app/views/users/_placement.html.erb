<div id="placement" >
	<div class="container">
		<div class="row overviewBox">
			<div class="tabOx">
				<div class="table-responsive">          
					<table class="table table-striped">
						<thead>
						  <tr>
							<th>STREAM NAME</th>
							<th>NO OF COMPANY VISITED</th>
							<th>NUMBER OF STUDENTS PLACED</th>
							<th>YEAR</th>
						  </tr>
						</thead>
						<tbody>
							<% if @placement.present? %>
								<% @placement.each do |place|%>
								  	<tr>
										<td><%= place.title%></td>
										<td><%= place.companies_visited%></td>
										<td><%= place.students_placed%></td>
										<td><%= place.year%></td>
								  	</tr>
							  	<%end%>
							<%else%>
								<tr>
									<td>No Data Yet</td>
									<td>No Data Yet</td>
									<td>No Data Yet</td>
									<td>No Data Yet</td>
								</tr>
							<%end%>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>